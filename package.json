{
    "private": true,
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "start": "cd frontend && react-scripts start",
        "build": "cd frontend && react-scripts build",
        "test": "jest",
        "eject": "cd frontend && react-scripts eject",
        "type-check": "tsc --noEmit"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "\\.(ts|tsx)$": "ts-jest"
        }
    }
}
