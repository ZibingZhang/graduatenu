{
  "name": "GraduateNU",
  "private": true,
  "engines": {
    "node": ">= 12.x"
  },
  "homepage": "https://graduatenu.com",
  "workspaces": [
    "frontend",
    "scrapers",
    "api"
  ],
  "proxy": "http://localhost:3001",
  "scripts": {
    "start": "cd infrastructure/develop && ./compose-build.sh && ./run-servers.sh dev-build",
    "prod-start": "cd infrastructure/develop && ./compose-build.sh && ./run-servers.sh prod-build",
    "build": "cd frontend && CI=false react-app-rewired build",
    "test": "jest",
    "eject": "cd frontend && react-app-rewired eject",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "husky": "^4.2.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {}
}
