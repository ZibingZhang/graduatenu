{
  "name": "frontend",
  "version": "1.0.0",
  "description": "A degree scheduling service for Northeastern students.",
  "dependencies": {
    "apollo-boost": "^0.4.4",
    "cross-env": "5.0.5",
    "graphql-tag": "^2.10.1",
    "react": "^16.9.0",
    "react-apollo": "^3.1.3",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-scripts": "^3.1.2",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/styled-components": "^4.1.19",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "type-check": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    }
  }
}
